import java.util.*;
public class newpatt {
    public static void main(String []args){
        int a=11;
        for(int i=0;i<a;i++){
            int h =1;
            for(int j=0;j<a;j++){
                if(i+j<=10-a/2||i+j>=10+a/2||i<=j-a/2||i>=j+a/2)
                System.out.print( "* ");
                else
                System.out.print("  ");
            }
            System.out.println();
        }

    }
    
}
