import java.util.*;
public class newpatt {
    public static void main(String []args){
        int a=11;
        for(int i=0;i<a;i++){
            int h =1;
            for(int j=0;j<a;j++){
                if(i>=j&&i+j<=10||i<=j&&i+j>=10)
                System.out.print( "* ");
                else
                System.out.print("  ");
            }
            System.out.println();
        }

    }
    
}
