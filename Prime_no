import java.util.*;

class Main {
    static Scanner sc = new Scanner(System.in);
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int a = sc.nextInt();
         int count=0;
        for(int i =1;i<=a;i++){
            
            if(a%i==0){
                count++;
            }
        }System.out.println(count);
    }
}
//java program to find prime no
