import java.util.*;
public class forloop {
    public static void main(String []args){
        int a=11;
        for(int i=0;i<a;i++){
            for(int j=0;j<a;j++){
                if(i==0||i==10||j==0||j==10||i==5||j==5||i==j||i+j==10)
                System.out.print("* ");
                else
                System.out.print("  ");
            }
            System.out.println();
        }

    }
    
}
